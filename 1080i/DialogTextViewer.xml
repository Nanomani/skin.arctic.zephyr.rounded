<?xml version="1.0" encoding="UTF-8"?>
<window id="147">
    <defaultcontrol>61</defaultcontrol>
    <controls>
        <include>Background_Overlay</include>

        <control type="group">

            <include content="Def_Dialog_Background"><param name="height" value="900" /><param name="width" value="1760" /></include>

            <control type="label" id="1">
                <include>Def_Dialog_Header_Title</include>
                <label>1026</label>
            </control>

            <include>Def_Dialog_Header_Logo</include>
            <include>Def_Dialog_Header_Line</include>

            <!-- TEXTBOX -->

            <control type="textbox" id="5">
                <description>textarea</description>
                <left>60</left>
                <right>86</right>
                <top>115</top>
                <bottom>70</bottom>
                <align>justify</align>
                <pagecontrol>61</pagecontrol>
            </control>

            <!-- SCROLLBAR  -->

            <include content="Object_Scrollbar_Dialog">
                <param name="scrollid" value="61"/>
